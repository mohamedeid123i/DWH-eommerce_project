                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
    mc.campaign_id,
    ISNULL(o.order_date, '9999-12-31') AS order_date,
    COUNT(DISTINCT o.order_id) AS total_orders,
    ISNULL(SUM(oi.subtotal), 0) AS total_revenue,
    ISNULL(SUM(oi.discount), 0) AS total_discount,
	o.last_update_date
FROM 
    marketing_campaigns mc
LEFT JOIN 
    orders o ON o.campaign_id = mc.campaign_id
LEFT JOIN 
    orderitem oi ON o.order_id = oi.order_id
where o.last_update_date &gt; (select max(last_extract_date) from STG.confg_table where table_name = 'F_marketing_effectiveness')
GROUP BY 
    mc.campaign_id, o.order_date,o.last_update_date</property>
